{
  "name": "@moneyfi/ts-sdk",
  "description": "MoneyFi TypeScript SDK",
  "packageManager": "pnpm@8.15.1",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "bugs": {
    "url": "https://github.com/MoneyFi-fund/moneyFi-SDK/issues/new/choose"
  },
  "homepage": "https://github.com/MoneyFi-fund/moneyFi-SDK/",
  "main": "dist/common/index.js",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "require": {
        "types": "./dist/common/index.d.ts",
        "default": "./dist/common/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.mts",
        "default": "./dist/esm/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build:clean": "rm -rf dist",
    "build": "pnpm build:clean && tsup",
    "prepublishOnly": "pnpm run build",
    "_fmt": "prettier 'src/**/*.ts' 'tests/**/*.ts' 'eslint.config.cjs'",
    "fmt": "pnpm _fmt --write",
    "lint": "eslint '**/*.{cts,mts,ts}'",
    "test": "pnpm jest",
    "unit-test": "pnpm jest tests/unit",
    "e2e-test": "pnpm jest tests/e2e",
    "doc": "scripts/generateDocs.sh",
    "spec": "pnpm build && pnpm _spec",
    "_spec": "cucumber-js -p default"
  },
  "dependencies": {
    "axios": "^1.12.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/base-64": "^1.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.25",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.39.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.1.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "^2.26.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typedoc": "^0.28.1",
    "typedoc-plugin-missing-exports": "^4.0.0",
    "typescript": "^5.8.2"
  },
  "version": "0.1.2",
  "publishConfig": {
    "access": "public"
  }
}
